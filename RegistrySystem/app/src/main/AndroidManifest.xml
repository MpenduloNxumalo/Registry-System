<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.example.registrysystem" >

	<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

	<application
		android:allowBackup="true"
		android:theme="@style/AppTheme"
		android:label="@string/app_name" >
		<activity
		android:name=".ClientScreens.homescreen_activity"
		android:configChanges="orientation|keyboardHidden|screenSize"
		android:label="@string/app_name"
		android:exported="true"  >
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

			<activity android:name="com.example.registrysystem.ClientScreens.clockinlistscreen_activity"></activity>
			<activity android:name="com.example.registrysystem.ClientScreens.createregisterscreen_activity"></activity>
			<activity android:name="com.example.registrysystem.ClientScreens.createregister2screen_activity"></activity>
			<activity android:name="com.example.registrysystem.AdminScreens.exportscreen_activity"></activity>
			<activity android:name="com.example.registrysystem.ClientScreens.donescreen_activity"></activity>
	</application>

</manifest>
	
	